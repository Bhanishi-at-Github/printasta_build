{
  "version": 2,
  "name": "printasta",
  "builds": [
    {
      "src": "manage.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "30mb"
      }
    },
    {
      "src": "/static/(.*)",
      "use": "@vercel/static",
      "config": {
        "distDir": "/static/"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ]
}