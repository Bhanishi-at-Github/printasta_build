{
  "version": 2,
  "name": "printasta",
  "builds": [
    {
      "src": "printasta/wsgi.py",
      "use": "@ardnt/now-python-wsgi",
      "config": {
        "maxLambdaSize": "30mb",
        "runtime": "python3.11.4"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "main/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "printasta/wsgi.py"
    }
  ]
}