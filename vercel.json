{
  "version": 2,
  "name": "printasta",
  "builds": [
    {
      "src": "printasta/wsgi.py",
      "use": "@natanffs/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "30mb"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static",
      "config": {
        "distDir": "main/static"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "printasta/wsgi.py"
    }
  ]
}